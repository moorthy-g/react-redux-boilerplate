{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-css-modules",{
          "generateScopedName": "[local]_[hash:base64:5]",
          "filetypes": {
            ".less": {
              "syntax": "postcss-less"
             }
          }
        }]
      ]
    },
    "production": {
      "plugins": [
        ["react-css-modules",{
          "generateScopedName": "[hash:base64:5]",
          "filetypes": {
            ".less": {
              "syntax": "postcss-less"
             }
          }
        }]
      ]
    }
  }
}
