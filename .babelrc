{
  "presets": [
    [ "es2015", { "modules": false } ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import"
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-css-modules",{
          "generateScopedName": "[local]_[hash:base64:5]"
        }]
      ]
    },
    "production": {
      "plugins": [
        ["react-css-modules",{
          "generateScopedName": "[hash:base64:10]"
        }]
      ]
    }
  }
}
